{
  "name": "NAPCOM",
  "version": "0.2.0",
  "author": {
    "name": "Tom Stave"
  },
  "keywords": [
    "angular",
    "angular 12",
    "electron",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "package-mac": "electron-packager . napcom-app --overwrite --asar=true --platform=darwin --arch=x64 --icon=assets/icons/favicon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . napcom-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"NAPCOM\"",
    "package-linux": "electron-packager . napcom-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/256x256.png --prune=true --out=release-builds"
  },
  "dependencies": {
    "@angular/animations": "13.2.2",
    "@angular/cdk": "13.2.2",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/compiler-cli": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "13.2.2",
    "@angular/language-service": "13.2.2",
    "@angular/material": "13.2.2",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/platform-server": "13.2.2",
    "@angular/router": "13.2.2",
    "@electron/remote": "2.0.4",
    "@material/typography": "^13.0.0",
    "bootstrap": "5.1.3",
    "core-js": "3.21.0",
    "csvtojson": "2.0.10",
    "font-awesome": "4.7.0",
    "fs-extra": "10.0.0",
    "material-icons": "1.10.6",
    "node-xlsx": "0.21.0",
    "npm-check-updates": "12.3.0",
    "rxjs": "7.5.4",
    "tslib": "2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "13.2.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.2.3",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "chai": "4.3.6",
    "conventional-changelog-cli": "2.2.2",
    "cross-env": "7.0.3",
    "electron": "13.6.9",
    "electron-builder": "22.14.13",
    "electron-packager": "15.4.0",
    "electron-reload": "1.5.0",
    "eslint": "8.9.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.9.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "4.0.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.16",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.1.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "mocha": "9.2.0",
    "nan": "2.15.0",
    "npm-run-all": "4.1.5",
    "spectron": "19.0.0",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "wait-on": "6.0.1",
    "webdriver-manager": "12.1.8"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "chrome 83"
  ]
}
